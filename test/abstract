		$(function(){
			$("#citeproc-tabs .tabbed-content").eq(0).html("Choose a citation style from the tabs above.").show();

			$("#citeproc-tabs li a").click(function(event){
				if (!citation_data) citation_data = transformJSON(citation_json);

				var currentTab = $("#citeproc-tabs li a").index(event.target);

				$("#citeproc-tabs .tabbed-content:visible").hide();
				$("#citeproc-tabs li a").removeClass("active").eq(currentTab).addClass("active");

		  		var name = $(this).text();
				var citeproc = new CSL.Engine(citeproc_sys, citeproc_styles[name]);
				citeproc.updateItems(["ITEM-1"]);
				var mybib = citeproc.makeBibliography();

				if (mybib && mybib.length && mybib[1].length) {
					$("#citeproc-tabs .tabbed-content").eq(currentTab).html(mybib[1].join("")).show();
				}
			});
		});
